@model BackendProj.Models.Teacher

<h1>Dashboard Nauczyciela</h1>
<p>Witaj, @Model.Name @Model.Surname</p>

<h2>Twoi uczniowie:</h2>
@foreach (var subject in Model.Subjects)
{
<h3>@subject.Name</h3>
<ul>
    @foreach (var grade in subject.Grades)
    {
    <li>@grade.Student.Name @grade.Student.Surname - Ocena: @grade.Value</li>
    }
</ul>
}